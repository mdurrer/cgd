OBJECT hodzid
title Stuhl
  ROOM hod
  PRIORITY 190
  STATUS on
  SEQ ..\HODOVNA\DEF\zid_oper
  INIT
    load hodzid "mlci"
    start hodzid "mlci"
  GPLEND
  lookc 40 180 vpravo
  look
   labels zidlookok
    if(isobjon(hodzid_dobra)) zidlookok
    justtalk
D: "Die Polsterung ist aufgerissen."
    juststay
    exit
   label zidlookok
    justtalk
D: "Jetzt wo es genäht wurde,
    sieht er besser aus..."
    juststay
  gplend
  canuse ((isactico(i_cihla)and (nedosahnu_na_pohadku=1)and isobjon(pohadka)and isobjon(hodzid_dobra)) or isactico(0) or isactico(i_jehla) or isactico(i_nit) or isactico(i_zaplata) )
  usec 40 180 vpravo
  use
   labels zidroz mamevse kljeh klnit zapoba zapnit jehoba jehnit nitoba nitjeh
    if(isactico(0)or isactico(i_cihla)) zidroz
    if(isicoon(i_jehla)and isicoon(i_nit) and isicoon(i_zaplata) ) mamevse
    if(isactico(i_jehla)) kljeh
    if(isactico(i_nit)) klnit

    {pouzivame zaplatu:}
       justtalk
D: "Ich hab den Flicken..."
       juststay
       if(not isicoon(i_jehla)and not isicoon(i_nit)) zapoba
       if(not isicoon(i_nit)) zapnit
      {nemame jehlu:}
        justtalk
D: "...aber ich hab noch keine Nadel."
        juststay
        exit
      label zapnit
      {nemame nit:}
        justtalk
D: "...aber ich hab noch keinen Faden."
        juststay
        exit
      label zapoba
      {nemame nit i jehlu:}
        justtalk
D: "...aber ich hab weder Nadel noch Faden.
    Wo könnte ich die her bekommen?"
        juststay
        exit

    label kljeh
    {pouzivame jehlu:}
       justtalk
D: "Ich hab eine Nadel..."
       juststay
       if(not isicoon(i_zaplata)and not isicoon(i_nit)) jehoba
       if(not isicoon(i_nit)) jehnit
      {nemame zaplatu:}
        justtalk
D: "...aber ich hab noch keinen Flicken."
        juststay
        exit
      label jehnit
      {nemame nit:}
        justtalk
D: "...aber ich hab noch keinen Faden."
        juststay
        exit
      label jehoba
      {nemame nit i zaplatu:}
        justtalk
D: "...aber ich hab weder Faden noch Flicken.
    Wo könnte ich die her bekommen?"
        juststay
        exit

    label klnit
    {pouzivame nit:}
       justtalk
D: "Ich hab ein Stück Faden..."
       juststay
       if(not isicoon(i_zaplata)and not isicoon(i_jehla)) nitoba
       if(not isicoon(i_jehla)) nitjeh
      {nemame zaplatu:}
        justtalk
D: "...aber ich hab noch keinen Flicken."
        juststay
        exit
      label nitjeh
      {nemame jehlu:}
        justtalk
D: "...aber ich hab noch keine Nadel."
        juststay
        exit
      label nitoba
      {nemame nit i zaplatu:}
        justtalk
D: "...aber ich hab weder Nadel noch Flicken.
    Wo könnte ich die wohl her bekommen?"
        juststay
        exit

    label mamevse
    {zasivame zidli:}
      icostat off i_jehla
      icostat off i_nit
      icostat off i_zaplata
      justtalk
D: "Mit dem Spinnenfaden
    und dem Igelstachel
    kann ich die Kleidung des Zwergen
    als Flicken benutzen."
D: "Schade das ich mich nicht bei ihm bedanken kann."
D: "Wir wären sicher gute Freunde geworden..."
      juststay
      walkonplay 44 196 vpravo
      load bert "sije"
      objstat away hodzid_blba
      objstat_on hodzid_dobra hod
      load hodzid_dobra "sijem"
      start hodzid_dobra "sijem"
      startplay bert "sije"
      start hodzid_dobra "z"
      start bert "kour-pravo"
      exit


    label zidroz
    {obycejny rozhovor:}
     load hodzid "mluvi"
     dialogue "zidle.txt"
  gplend

