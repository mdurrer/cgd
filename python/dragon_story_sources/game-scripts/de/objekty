   labels levitace_ne
    if (tereza_levituje<=0) levitace_ne
    let tereza_levituje (tereza_levituje+1)
    if (tereza_levituje<5) levitace_ne
    {prave popate budeme levitovat VZHURU NOHAMA:}
    load mala_prava "extra"
    start mala_prava "extra"
    exit
   label levitace_ne
    load mala_prava "ZAKLADNI"
    start mala_prava "ZAKLADNI"
  GPLEND
  look
   labels skip
    justtalk
D: "Sie ist genau mein Typ..."
    goto skip
D: "Könnte dieser Stuhl das Nirvana erfahren?"
D: "Die Antwort zu der Frage such ich schon
    seitdem ich denken kann."
   label skip
D: "Ganz davon zu schweigen, daß dieser Stuhl
    allen Gesetzen der Optik trotzt!"
    juststay
  gplend

