object hu_louc1
title Fackel
  room hraduvn
  priority 2
  status on
  seq ..\hrad_in\def\louc1
  init
    load hu_louc1 "hori"
    start hu_louc1 "hori"
  gplend
  look
    justtalk
D: "F-A-C-K-E-L"
D: "Verstanden?"
    juststay
  gplend

