OBJECT lenuvnitr_hlava
title Liegt wach, schluckt
  ROOM LenUvnitr
  PRIORITY 73
  STATUS On
  SEQ ..\LENOCH_I\DEF\hlava
  INIT
    load lenuvnitr_hlava "mlci"
    start lenuvnitr_hlava "mlci"
  GPLEND
  canuse (isactico(0))
  usec 201 169 vlevo
  use
    execuse lenoch
  gplend
  look
    execlook lenoch
  gplend

