object kronika
title Chronik
  room skreti
  priority 5
  status on
  seq ..\skreti\def\kronika
  init
    load kronika "mlci"
    start kronika "mlci"
  gplend
  look
   labels kr_look_mluvi
    justtalk
    if (kronika_zdrava=1) kr_look_mluvi
D: "Sie spricht immer noch nicht."
    juststay
    exit
   label kr_look_mluvi
D: "Sie spricht wieder."
    juststay
  gplend
  canuse (isobjaway(o_pohrabac) and (isactico(0) or isactico(i_bylina)))
  usec 184 162 vpravo
  use
   labels kr_mluvi kr_lecim
    {bylinou uzdravime:}
    if (isactico(i_bylina)) kr_lecim
    {jinak se budeme snazir pokecat:}
    if (kronika_zdrava=1) kr_mluvi
    justtalk
D: "Frau Chronik!"
    juststay
D: "???"
    justtalk
D: "Sie bleibt stumm..."
    juststay
    exit
   label kr_mluvi
    load kronika "mluvi"
    dialogue "kronika.txt"
    start kronika "mlci"
    exit
   label kr_lecim
    let kronika_zdrava (1)
    load skreti_jiskry "jiskreni"
    startplay skreti_jiskry "jiskreni"
    objstat away skreti_jiskry
    icostat off i_bylina
    load kronika "mluvi"
    dialogue "kronika.txt"
    start kronika "mlci"
  gplend

