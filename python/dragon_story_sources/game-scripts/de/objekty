OBJECT lahvicka
title Fläschchen
  ROOM LenUvnitr
  PRIORITY 33
  STATUS ON
  SEQ ..\LENOCH_I\DEF\lahvicka
  INIT
    load lahvicka "ZAKLADNI"
    start lahvicka "ZAKLADNI"
  GPLEND
  look
   labels lah_lenost
    if(vim_o_lenosti>0) lah_lenost
    justtalk
D: "Es könnte Rasierwasser sein"
D: "oder Jod."
    juststay
    exit
   label lah_lenost
    justtalk
D: "Ich glaub, da ist Faulheit drin."
    juststay
  gplend
  canuse (isactico(0)and(stroj_funguje=3))
  use
    justtalk
D: "Und wir haben sie..."
    juststay
    icostat on i_lenost
    objstat away lahvicka
  gplend

