OBJECT p_spin_1
title Haufen Schrott
  ROOM jesuvn
  PRIORITY 162
  STATUS Off
  SEQ ..\JES_IN\DEF\p_spin_1
  INIT
    load p_spin_1 "ZAKLADNI"
    start p_spin_1 "ZAKLADNI"
  GPLEND
  lookc 76 160 vpravo
  look
   labels pokl_prst
    if (nasel_prsten=0) pokl_prst
    justtalk
D: "Anscheinend ein Haufen Metallschrott."
D: "Natürlich nimmt man ihn
    nur auf den ersten Blick,
    als einen Haufen Schrott wahr."
D: "Nur du und ich wissen,
    welch furchtbares Geheimnis er verbirgt..."
D: "Diese grausige Tatsache
    darf diese Höhle nie verlassen!"
    juststay
    exit
   label pokl_prst
    justtalk
D: "Ich könnte etwas vom Stapel nehmen.
    Nur ein kleines Souvenir."
    juststay
    let nasel_prsten (1)
    icostat on i_prsten
  gplend

