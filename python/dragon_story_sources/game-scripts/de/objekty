{************************ SipDumUvn *********************************}
object sd_kronika
title Chronik
  room sipdumuvn
  priority 142
  status on
  seq ..\sipdum_i\def\sd_kron
  init
    load sd_kronika "zakladni"
    start sd_kronika "zakladni"
  gplend
  look
    justtalk
D: "Ein echter Hingucker."
D: "Ich hätte wirklich nicht gedacht,
    daß sie so ein schmuddeliges,
    fettes Taschenbuch sein würde..."
D: "...das hat mich echt überrascht!"
    juststay
  gplend
  canuse (isactico(0))
  use
    justtalk
D: "Chronik?"
D: "Komm schon..."
D: "Vielleicht sollte ich lispeln,
    damit du mit mir sprichst?"
D: "Stell dich nicht so an und hau rein!"
D: "Ich hoffe, du bist nicht taubstumm."
D: "(Vielleicht ist ihr iPod einfach zu laut...)"
    juststay
    let vim_o_kronice (1)
  gplend

object stopy1
title Fußabdruck
  room sipdumuvn
  priority 10
  status off
  seq ..\sipdum_i\def\stopy1
  init
    load stopy1 "zakladni"
    start stopy1 "zakladni"
  gplend
  look
   labels povykricniku
    if (prozkoumal_vykricnik>1) povykricniku
    justtalk
D: "Wem gehören die wohl?"
    juststay
    exit
   label povykricniku
    justtalk
D: "Ich glaube, die Fußspuren
    weisen auf eine Verbindung
    zu den Schmierereien hin."
D: "Aber ich bin nicht sicher!"
    juststay
  gplend

