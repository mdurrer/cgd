OBJECT hraduvn_zavrdve
title Tür
  ROOM hraduvn
  PRIORITY 153
  STATUS ON
  SEQ ..\HRAD_IN\DEF\EX_HODza
  INIT
    load hraduvn_zavrdve "ZAKLADNI"
    start hraduvn_zavrdve "ZAKLADNI"
{   loadmap "..\hrad_in\def\ex_hodza.map"
  GPLEND
  look
    justtalk
D: "Sie ist zu."
D: "Was soll ich mit ihr machen?"
    juststay
  gplend
  canuse (isactico(0))
  use
   labels hrdv_znamheslo hrdv_nepoprve
    if (hradni_dvere=4) hrdv_znamheslo
    justtalk
D: "Zu schade, sie lässt sich nicht öffnen."
    juststay
    if (hradni_dvere>0) hrdv_nepoprve
    let hradni_dvere (1)
    exit
   label hrdv_znamheslo
    justtalk
D: "Ich hoffe, ich hab's nicht vergessen..."
D: "Memoriter - Menander - Mendacium."
    objstat away hraduvn_zavrdve
D: "Hey, es hat funktioniert!"
{    roommap
    juststay
    exit
   label hrdv_nepoprve
  gplend


