object sd_kronika
  title kronika
  room sipdumuvn
  priority 142
  status on
  seq ..\sipdum_i\def\sd_kron
  init
    load sd_kronika "zakladni"
    start sd_kronika "zakladni"
  gplend
  look
    justtalk
    D: "Fešanda."
    D: "Ne že bych si myslel,
        že to bude zašlej umaštěnej pejprback..."
    D: "...ale tohle mě překvapilo!"
    juststay
  gplend
  canuse (isactico(0))
  use
    justtalk
    D: "Kroniko?"
    D: "No tak..."
    D: "Mám si snad začít šlapat na jazyk,
        abys se mnou mluvila?!"
    D: "Nedělěj fóry a pěkně se mnou spolupracuj!"
    D: "Doufám, že nejsi hluchoněmá."
    D: "(Třeba má jenom hlasitě wólkmena...)"
    juststay
    let vim_o_kronice (1)
  gplend

object stopy1
  title stopa
  room sipdumuvn
  priority 10
  status off
  seq ..\sipdum_i\def\stopy1
  init
    load stopy1 "zakladni"
    start stopy1 "zakladni"
  gplend
  look
   labels povykricniku
    if (prozkoumal_vykricnik>1) povykricniku
    justtalk
    D: "To by mě zajímalo,
        komu tak asi můžou patřit!"
    juststay
    exit
   label povykricniku
    justtalk
    D: "Něco mi našeptává, že ty stopy
        budou mít nějakou souvislost s těmi vykřičníky."
    D: "Ale nevím to jistě!"
    juststay
  gplend

