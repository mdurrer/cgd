OBJECT p_cist_1
  TITLE poklad
  ROOM jesuvn
  PRIORITY 163
  STATUS ON
  SEQ ..\JES_IN\DEF\p_cist_1
  INIT
    load p_cist_1 "ZAKLADNI"
    start p_cist_1 "ZAKLADNI"
  GPLEND
  canuse (isactico(i_saze))
  usec 76 160 vpravo
  use
   labels nemuze
    if(slepene_nohy!=1) nemuze
    icostat off i_saze
    objstat away jesuvn_lesk
    objstat_on  sazeleti jesuvn
    load sazeleti "leti"
    startplay sazeleti "leti"
    objstat away p_cist_1
    objstat away p_cist_2
    objstat away p_cist_3
    objstat_on  p_spin_1 jesuvn
    objstat_on  p_spin_2 jesuvn
    objstat_on  p_spin_3 jesuvn
    start p_spin_1 "ZAKLADNI"
    start p_spin_2 "ZAKLADNI"
    start p_spin_3 "ZAKLADNI"
    objstat away sazeleti
    exit
   label nemuze
    justtalk
    D: "Zašpinit poklad?"
    D: "To nemůžu otci před očima udělat!"
    juststay
  gplend
  lookc 76 160 vpravo
  look
   labels pokl_prst
    justtalk
    if (nasel_prsten=0) pokl_prst
    D: "Fíha!"
    D: "Ne že bych na bohatství přímo lpěl,
        ale začínám otce tak trochu chápat."
    D: "Ale úkol je úkol..."
    D: "Rozhod' jsem se,
        že budu až do smrti žebrák."
    juststay
    exit
   label pokl_prst
    D: "Mohl bych si odtud odnést
        nějaký malý suvenýr."
    juststay
    let nasel_prsten (1)
    icostat on i_prsten
  gplend

