{nebyla kontrolována duplicita, nutno smazat příp. předch. výskyt}
OBJECT obj_dutina_maska168
  TITLE dutina
  ROOM dutina
  PRIORITY 168
  STATUS ON
  SEQ ..\DUTINA\DEF\DUT_DIRA
  INIT
    load obj_dutina_maska168 "ZAKLADNI"
    start obj_dutina_maska168 "ZAKLADNI"
  GPLEND
  lookc 160 180 vlevo
  look
   labels lookdut_2 lookdut_3 lookdut_4 lookdut_5 lookdut_6
    let dutina_prozkoumal (dutina_prozkoumal+1)
    if (dutina_prozkoumal=2) lookdut_2
    if (dutina_prozkoumal=3) lookdut_3
    if (dutina_prozkoumal=4) lookdut_4
    if (dutina_prozkoumal=5) lookdut_5
    if (dutina_prozkoumal>5) lookdut_6
    {teprve po prve:}
    justtalk
    D: "To je moje tajná skrýš."
    juststay
    exit
   label lookdut_2
    justtalk
    D: "Á, vida, co ukrývala. Prak!"
    juststay
    icostat on i_prak
    exit
   label lookdut_3
    let dutina_prozkoumal (4)
    goto lookdut_4
    justtalk
    D: "Pak už tam jsou jenom hambaté časopisy."
    juststay
    exit
   label lookdut_4
    justtalk
    D: "Vážně vám říkám,
        že tam už nic dalšího nebude."
    juststay
    exit
   label lookdut_5
    justtalk
    D: "Ale přece jenom, doutník!
        Důslednost se vyplatila..."
    juststay
    icostat on i_doutnik
    exit
   label lookdut_6
    justtalk
    D: "Nic tam není. Říkám vám to zcela vážně."
    juststay
  gplend


