OBJECT hodzid
  TITLE židle
  ROOM hod
  PRIORITY 190
  STATUS on
  SEQ ..\HODOVNA\DEF\zid_oper
  INIT
    load hodzid "mlci"
    start hodzid "mlci"
  GPLEND
  lookc 40 180 vpravo
  look
   labels zidlookok
    if(isobjon(hodzid_dobra)) zidlookok
    justtalk
    D: "Má poškozené čalounění."
    juststay
    exit
   label zidlookok
    justtalk
    D: "Jak jsem ji zašil,
        vypadá hned o trochu líp..."
    juststay
  gplend
  canuse ((isactico(i_cihla)and (nedosahnu_na_pohadku=1)and isobjon(pohadka)and isobjon(hodzid_dobra)) or isactico(0) or isactico(i_jehla) or isactico(i_nit) or isactico(i_zaplata) )
  usec 40 180 vpravo
  use
   labels zidroz mamevse kljeh klnit zapoba zapnit jehoba jehnit nitoba nitjeh
    if(isactico(0)or isactico(i_cihla)) zidroz
    if(isicoon(i_jehla)and isicoon(i_nit) and isicoon(i_zaplata) ) mamevse
    if(isactico(i_jehla)) kljeh
    if(isactico(i_nit)) klnit

    {pouzivame zaplatu:}
       justtalk
       D: "Záplatu bych měl..."
       juststay
       if(not isicoon(i_jehla)and not isicoon(i_nit)) zapoba
       if(not isicoon(i_nit)) zapnit
      {nemame jehlu:}
        justtalk
        D: "...ale chybí mi ještě jehla."
        juststay
        exit
      label zapnit
      {nemame nit:}
        justtalk
        D: "...ale chybí mi ještě nit."
        juststay
        exit
      label zapoba
      {nemame nit i jehlu:}
        justtalk
        D: "...ale chybí k ní nit a jehla!"
        juststay
        exit

    label kljeh
    {pouzivame jehlu:}
       justtalk
       D: "Jehlu bych měl..."
       juststay
       if(not isicoon(i_zaplata)and not isicoon(i_nit)) jehoba
       if(not isicoon(i_nit)) jehnit
      {nemame zaplatu:}
        justtalk
        D: "...ale chybí mi ještě záplata."
        juststay
        exit
      label jehnit
      {nemame nit:}
        justtalk
        D: "...ale chybí mi ještě nit."
        juststay
        exit
      label jehoba
      {nemame nit i zaplatu:}
        justtalk
        D: "...ale chybí k ní nit a záplata!"
        juststay
        exit

    label klnit
    {pouzivame nit:}
       justtalk
       D: "Nit bych měl..."
       juststay
       if(not isicoon(i_zaplata)and not isicoon(i_jehla)) nitoba
       if(not isicoon(i_jehla)) nitjeh
      {nemame zaplatu:}
        justtalk
        D: "...ale chybí mi ještě záplata."
        juststay
        exit
      label nitjeh
      {nemame jehlu:}
        justtalk
        D: "...ale chybí mi ještě jehla."
        juststay
        exit
      label nitoba
      {nemame nit i zaplatu:}
        justtalk
        D: "...ale chybí k ní jehla a záplata!"
        juststay
        exit

    label mamevse
    {zasivame zidli:}
      icostat off i_jehla
      icostat off i_nit
      icostat off i_zaplata
      justtalk
      D: "Nití od mého kamaráda pavouka
          a jehlou od důvěrného přítele ježka
          přišiju záplatu od přítele trpaslíka."
      D: "Škoda, že mu nemůžu poděkovat osobně."
      D: "Vsadím se, že bychom si dobře rozuměli..."
      juststay
      walkonplay 44 196 vpravo
      load bert "sije"
      objstat away hodzid_blba
      objstat_on hodzid_dobra hod
      load hodzid_dobra "sijem"
      start hodzid_dobra "sijem"
      startplay bert "sije"
      start hodzid_dobra "z"
      start bert "kour-pravo"
      exit


    label zidroz
    {obycejny rozhovor:}
     load hodzid "mluvi"
     dialogue "zidle.txt"
  gplend

