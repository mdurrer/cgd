    exit
   label par4_par
    execuse par
  gplend
  lookc 68 160 vpravo
  look
   labels lookpar4_par
    if(isobjon(par)) lookpar4_par
    execlook par0
    exit
   label lookpar4_par
    execlook par
  gplend

