    objstat_on chaven_dvere chaven
    load chaven_dvere "otvirani"
    load bert "kope"
    start chaven_dvere "otvirani"
    startplay bert "kope"
    start chaven_dvere "otevrene"
    load bert "dobrej-pravo"
    startplay bert "dobrej-pravo"
    start bert "kour-pravo"
  gplend

