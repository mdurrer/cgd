OBJECT sipdumven_list_down
  TITLE list
  ROOM sipdumven
  PRIORITY 10
  STATUS off
  SEQ ..\SIPDUM_O\DEF\list_dwn
  INIT
    load sipdumven_list_down "ZAKLADNI"
    start sipdumven_list_down "ZAKLADNI"
  GPLEND
  canuse (isactico(0))
  use
    objstat away sipdumven_list_down
    icostat on i_list
  gplend
  look
   labels skip
    justtalk
    D: "Když se škube krocan, lítá kolem peří."
    D: "Vážně pochybuju o tom,
        že takové přísloví existuje."
    D: "Ale chci tím vyjádřit moji lítost,
        že jsem tomu lístečkovi ublížil."
    D: "Vždyť on spadnul ze stromu!"
    D: "Copak ho teď asi čeká?"
    D: "Asi uschne."
    goto skip
    D: "Dá se to říct taky tak,
        že je teď vlastně mrtvý."
    D: "Ne!!!"
    D: "Co jsem to probůh provedl!!!"
    D: "Takové drsné setkání se smrtí..."
    D: "...může snadno přivodit
        mladé nevyvinuté dračí bytosti,
        jako jsem já..."
    D: "...závážný psychický problém!"
    D: "Rychle odtud pryč!"
    D: "Zamést stopy, honem!"
    D: "(O to se možná postará první poryv větru...)"
    D: "Anebo neměl bych se snad pokusit
        vypravit mu menší obřad?"
    D: "Nic velkého a drahého, chápejte, ale přesto..."
    D: "Podívejte, jak tu důstojně leží..."
   label skip
    D: "Mohl bych ho taky zkusit nějak zrecyklovat."
    D: "To je ono, pryč s falešnou lítostí,
        jaké štěstí, že ten list spadnul,
        může mi být ještě na něco užitečný!"
    juststay
  gplend

