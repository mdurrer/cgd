OBJECT mala_prava
  TITLE levitující|židle
  ROOM zid
  PRIORITY 163
  STATUS ON
  SEQ ..\ZIDLE\DEF\zid_prav
  INIT
   labels levitace_ne
    if (tereza_levituje<=0) levitace_ne
    let tereza_levituje (tereza_levituje+1)
    if (tereza_levituje<5) levitace_ne
    {prave popate budeme levitovat VZHURU NOHAMA:}
    load mala_prava "extra"
    start mala_prava "extra"
    exit
   label levitace_ne
    load mala_prava "ZAKLADNI"
    start mala_prava "ZAKLADNI"
  GPLEND
  look
   labels skip
    justtalk
    D: "Přesně můj typ."
    goto skip
    D: "Může židle prožívat nirvánu?"
    D: "Na tuto otázku hledám odpověď od doby,
        co jsem vyšel z lůna matky."
   label skip
    D: "Tahleta židle navíc popírá
        veškeré zákony optiky!"
    juststay
  gplend

