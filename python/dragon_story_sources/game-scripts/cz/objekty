object lenuvnitr_buchta
  title
  room lenuvnitr
  priority 74
  status on
  seq ..\lenoch_i\def\buchta
  init
   labels buchta_ok
    load lenuvnitr_buchta "dobre"
    if(stroj_funguje>=2) buchta_ok
