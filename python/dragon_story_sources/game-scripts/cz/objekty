object vykricnik
  title vykřičník
  room sipdumuvn
  priority 10
  status off
  seq ..\sipdum_i\def\vykric
  init
    load vykricnik "zakladni"
    start vykricnik "zakladni"
  gplend
  look
   labels vy_podruhe vy_potreti vy_poctvrte vy_popate
    if (prozkoumal_vykricnik=1) vy_podruhe
    if (prozkoumal_vykricnik=2) vy_potreti
    if (prozkoumal_vykricnik=3) vy_poctvrte
    if (prozkoumal_vykricnik=4) vy_popate
    justtalk
    D: "Ten vykřičník mi připadá povědomý..."
    juststay
    let prozkoumal_vykricnik (1)
    exit
   label vy_podruhe
    justtalk
    D: "Tato jílovitoblátivá půda může pocházet jedině..."
    D: "...jedině odkud?!"
    juststay
    let prozkoumal_vykricnik (2)
    exit
   label vy_potreti
    justtalk
    D: "Podle těch stop těžko říct,
        ale podle těch vykřičníků bych soudil že..."
    D: "...pachatelé budou nedaleko!"
    juststay
    let prozkoumal_vykricnik (3)
    exit
   label vy_poctvrte
    justtalk
    D: "No jasně, už vím!"
    D: "Takové vykřičníky totiž můžou být na jediném místě!"
    D: "Temný les na severozápadě!"
    juststay
    let new_skreti (1)
    let prozkoumal_vykricnik (4)
    exit
   label vy_popate
    justtalk
    D: "A hele, náš starý známý vykřičník."
    juststay
  gplend

