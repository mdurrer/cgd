ICON i_provaz
  TITLE provaz
  STATUS off
  PICTURE ..\ikony\def\07.gcf
  look
    D: "Pevný a tenký."
  gplend
  canuse (isactico(i_varecka_zlomena))
  use
    D: "Vařečku vyspravíme provázkem..."
    icostat off i_varecka_zlomena
    icostat off i_provaz
    icostat on i_varecka
  gplend

ICON i_varecka_zlomena
  TITLE zlomená vařečka
  STATUS off
  PICTURE ..\ikony\def\08.gcf
  look
    D: "Tyhle dva kousky dřeva mi na nic nebudou."
  gplend
  canuse (isactico(i_provaz))
  use
    D: "Provázkem vařečka půjde vyspravit..."
    icostat off i_varecka_zlomena
    icostat off i_provaz
    icostat on i_varecka
  gplend

