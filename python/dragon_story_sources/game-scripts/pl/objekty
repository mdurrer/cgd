{nebyla kontrolována duplicita, nutno smazat příp. předch. výskyt}
OBJECT obj_dutina_maska168
   title dziupla
  ROOM dutina
  PRIORITY 168
  STATUS ON
  SEQ ..\DUTINA\DEF\DUT_DIRA
  INIT
    load obj_dutina_maska168 "ZAKLADNI"
    start obj_dutina_maska168 "ZAKLADNI"
  GPLEND
  lookc 160 180 vlevo
  look
   labels lookdut_2 lookdut_3 lookdut_4 lookdut_5 lookdut_6
    let dutina_prozkoumal (dutina_prozkoumal+1)
    if (dutina_prozkoumal=2) lookdut_2
    if (dutina_prozkoumal=3) lookdut_3
    if (dutina_prozkoumal=4) lookdut_4
    if (dutina_prozkoumal=5) lookdut_5
    if (dutina_prozkoumal>5) lookdut_6
    {teprve po prve:}
    justtalk
   D: "To jest moja sekretna kryjówka."
    juststay
    exit
   label lookdut_2
    justtalk
   D: "Och, patrz co tu jest schowane. Proca!"
    juststay
    icostat on i_prak
    exit
   label lookdut_3
    let dutina_prozkoumal (4)
    goto lookdut_4
    justtalk
   D: "Poza tym są tam też pewne
       nieprzyzwoite czasopisma."
    juststay
    exit
   label lookdut_4
    justtalk
   D: "Zapewniam cię,
       że nic tam więcej nie ma."
    juststay
    exit
   label lookdut_5
    justtalk
   D: "Lecz jeszcze - cygaro!
       To tworzy pewną całość..."
    juststay
    icostat on i_doutnik
    exit
   label lookdut_6
    justtalk
   D: "Nic więcej tam nie ma. Mówię poważnie."
    juststay
  gplend


