OBJECT zebrak_mrt
   title żebrak
  ROOM hradven
  PRIORITY 20
  STATUS off
  SEQ ..\hrad_out\def\zebr_mrt
  init
    load zebrak_mrt "zakladni"
    start zebrak_mrt "zakladni"
  gplend
  lookc 152 196 vpravo
  look
   labels zebr_klika
    justtalk
   D: "Chudy jak szkielet."
    juststay
    if (je_tam_klika=1) zebr_klika
    exit
   label zebr_klika
    let je_tam_klika (0)
    icostat on i_klika
    justtalk
   D: "Och, co ja widzę, klamka do okna
       od nas z domu!"
    juststay
  gplend

