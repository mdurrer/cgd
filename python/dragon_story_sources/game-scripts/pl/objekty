{************************ SipDumUvn *********************************}
object sd_kronika
   title kronika
  room sipdumuvn
  priority 142
  status on
  seq ..\sipdum_i\def\sd_kron
  init
    load sd_kronika "zakladni"
    start sd_kronika "zakladni"
  gplend
  look
    justtalk
   D: "Kronika."
   D: "Nie sądziłem doprawdy,
       że to taka poplamiona, gruba książka
       w taniej papierowej oprawie..."
   D: "... to naprawdę mnie zaskoczyło!"
    juststay
  gplend
  canuse (isactico(0))
  use
    justtalk
   D: "Kroniko?"
   D: "A więc..."
   D: "Móze powinienem seplenić,
       żebyś zechciała ze mną rozmawiać?"
   D: "Nic nie ukrywaj i bądź ze mną szczera!"
   D: "Mam nadzieję, że nie jesteś głuchoniema."
   D: "(A może tylko ma wsadzony do środka
       magnetofon...)"
    juststay
    let vim_o_kronice (1)
  gplend

object stopy1
   title ślady stóp
  room sipdumuvn
  priority 10
  status off
  seq ..\sipdum_i\def\stopy1
  init
    load stopy1 "zakladni"
    start stopy1 "zakladni"
  gplend
  look
   labels povykricniku
    if (prozkoumal_vykricnik>1) povykricniku
    justtalk
   D: "Zastanawiam się czyje to ślady!"
    juststay
    exit
   label povykricniku
    justtalk
   D: "Zdaje mi się, że te ślady mogą mieć
       jakiś związek z tymi wykrzyknikami..."
   D: "Ale nie jestem pewien!"
    juststay
  gplend

