OBJECT p_cist_1
   title skarb
  ROOM jesuvn
  PRIORITY 163
  STATUS ON
  SEQ ..\JES_IN\DEF\p_cist_1
  INIT
    load p_cist_1 "ZAKLADNI"
    start p_cist_1 "ZAKLADNI"
  GPLEND
  canuse (isactico(i_saze))
  usec 76 160 vpravo
  use
   labels nemuze
    if(slepene_nohy!=1) nemuze
    icostat off i_saze
    objstat away jesuvn_lesk
    objstat_on  sazeleti jesuvn
    load sazeleti "leti"
    startplay sazeleti "leti"
    objstat away p_cist_1
    objstat away p_cist_2
    objstat away p_cist_3
    objstat_on  p_spin_1 jesuvn
    objstat_on  p_spin_2 jesuvn
    objstat_on  p_spin_3 jesuvn
    start p_spin_1 "ZAKLADNI"
    start p_spin_2 "ZAKLADNI"
    start p_spin_3 "ZAKLADNI"
    objstat away sazeleti
    exit
   label nemuze
    justtalk
   D: "Czy powinienem zniszczyć skarb?"
   D: "Nie mogę tego zrobić na oczach mego ojca!"
    juststay
  gplend
  lookc 76 160 vpravo
  look
   labels pokl_prst
    justtalk
    if (nasel_prsten=0) pokl_prst
   D: "Kilof!"
   D: "Nie zależy mi na bogactwie,
       ale zaczynam trochę lepiej
       rozumieć mojego ojca."
   D: "Ale obowiązek przede wszystkim..."
   D: "Podjąłem decyzję
       do końca życia będę żebrakiem."
    juststay
    exit
   label pokl_prst
   D: "Mogę sobie stąd coś wziąć,
       taką małą pamiątkę."
    juststay
    let nasel_prsten (1)
    icostat on i_prsten
  gplend

