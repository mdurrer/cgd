OBJECT hodzid
   title krzesło
  ROOM hod
  PRIORITY 190
  STATUS on
  SEQ ..\HODOVNA\DEF\zid_oper
  INIT
    load hodzid "mlci"
    start hodzid "mlci"
  GPLEND
  lookc 40 180 vpravo
  look
   labels zidlookok
    if(isobjon(hodzid_dobra)) zidlookok
    justtalk
   D: "Ma obicie w żałobny deseń."
    juststay
    exit
   label zidlookok
    justtalk
   D: "Odkąd je pozszywałem
       wygląda trochę lepiej..."
    juststay
  gplend
  canuse ((isactico(i_cihla)and (nedosahnu_na_pohadku=1)and isobjon(pohadka)and isobjon(hodzid_dobra)) or isactico(0) or isactico(i_jehla) or isactico(i_nit) or isactico(i_zaplata) )
  usec 40 180 vpravo
  use
   labels zidroz mamevse kljeh klnit zapoba zapnit jehoba jehnit nitoba nitjeh
    if(isactico(0)or isactico(i_cihla)) zidroz
    if(isicoon(i_jehla)and isicoon(i_nit) and isicoon(i_zaplata) ) mamevse
    if(isactico(i_jehla)) kljeh
    if(isactico(i_nit)) klnit

    {pouzivame zaplatu:}
       justtalk
   D: "zdobyłem łaty..."
       juststay
       if(not isicoon(i_jehla)and not isicoon(i_nit)) zapoba
       if(not isicoon(i_nit)) zapnit
      {nemame jehlu:}
        justtalk
   D: "...lecz jeszcze nie mam igły."
        juststay
        exit
      label zapnit
      {nemame nit:}
        justtalk
   D: "...i nie mam nic do wypchania."
        juststay
        exit
      label zapoba
      {nemame nit i jehlu:}
        justtalk
   D: "...nie mam nic do wypchania ani igły
       by je naprawić!"
        juststay
        exit

    label kljeh
    {pouzivame jehlu:}
       justtalk
   D: "Dostałem jakąś igłę..."
       juststay
       if(not isicoon(i_zaplata)and not isicoon(i_nit)) jehoba
       if(not isicoon(i_nit)) jehnit
      {nemame zaplatu:}
        justtalk
   D: "...ale jeszcze nie mam łaty."
        juststay
        exit
      label jehnit
      {nemame nit:}
        justtalk
   D: "...ani nic do wypchania."
        juststay
        exit
      label jehoba
      {nemame nit i zaplatu:}
        justtalk
   D: "... nie mam nic do wypchania ani łaty
       by je naprawić!"
        juststay
        exit

    label klnit
    {pouzivame nit:}
       justtalk
   D: "Mam czym je wypchać..."
       juststay
       if(not isicoon(i_zaplata)and not isicoon(i_jehla)) nitoba
       if(not isicoon(i_jehla)) nitjeh
      {nemame zaplatu:}
        justtalk
   D: "...ale jeszcze nie mam łaty."
        juststay
        exit
      label nitjeh
      {nemame jehlu:}
        justtalk
   D: "...i nie mam igły."
        juststay
        exit
      label nitoba
      {nemame nit i zaplatu:}
        justtalk
   D: "... lecz nie mam igły ani łaty by je naprawić!"
        juststay
        exit

    label mamevse
    {zasivame zidli:}
      icostat off i_jehla
      icostat off i_nit
      icostat off i_zaplata
      justtalk
   D: "Coś do wypchania dostanę od mojego
       kolegi pająka,
       a igłę od mojego przyjaciela jeża,
       mogę wszyć łatę od mego
       przyjaciela krasnoludka."
   D: "Szkoda, że nie mogę mu
       osobiście podziękować."
   D: "Założę się, że moglibyśmy być
       bardzo dobrymi przyjaciółmi..."
      juststay
      walkonplay 44 196 vpravo
      load bert "sije"
      objstat away hodzid_blba
      objstat_on hodzid_dobra hod
      load hodzid_dobra "sijem"
      start hodzid_dobra "sijem"
      startplay bert "sije"
      start hodzid_dobra "z"
      start bert "kour-pravo"
      exit


    label zidroz
    {obycejny rozhovor:}
     load hodzid "mluvi"
     dialogue "zidle.txt"
  gplend

