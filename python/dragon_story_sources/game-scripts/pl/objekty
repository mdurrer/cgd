OBJECT p_spin_1
   title kupa żelastwa
  ROOM jesuvn
  PRIORITY 162
  STATUS Off
  SEQ ..\JES_IN\DEF\p_spin_1
  INIT
    load p_spin_1 "ZAKLADNI"
    start p_spin_1 "ZAKLADNI"
  GPLEND
  lookc 76 160 vpravo
  look
   labels pokl_prst
    if (nasel_prsten=0) pokl_prst
    justtalk
   D: "Po prostu zwykła kupa żelastwa."
   D: "Naturalnie, na pierwszy rzut oka
       to tylko zwykły złom."
   D: "Tylko ty i ja wiemy
       jaka fantastyczna tajemnica się tu kryje..."
   D: "Te straszne rzeczy muszą na zawsze
       pozostać w ukryciu!"
    juststay
    exit
   label pokl_prst
    justtalk
   D: "Zabiorę stąd jakiś drobiazg
       na pamiątkę."
    juststay
    let nasel_prsten (1)
    icostat on i_prsten
  gplend

