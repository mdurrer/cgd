  canuse (isactico(0)or(isactico(i_jezek)and(budu_drbat=1))
  usec 68 160 vpravo
  use
   labels par4_par
    if(isobjon(par)) par4_par
    execuse par0
    exit
   label par4_par
    execuse par
  gplend
  lookc 68 160 vpravo
  look
   labels lookpar4_par
    if(isobjon(par)) lookpar4_par
    execlook par0
    exit
   label lookpar4_par
    execlook par
  gplend

