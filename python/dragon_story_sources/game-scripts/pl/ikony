ICON i_provaz
   title sznurek
  STATUS off
  PICTURE ..\ikony\def\07.gcf
  look
   D: "Jest mocny i cienki."
  gplend
  canuse (isactico(i_varecka_zlomena))
  use
   D: "Sznurkiem możemy naprawić
       drewnianą łyżkę kuchenną..."
    icostat off i_varecka_zlomena
    icostat off i_provaz
    icostat on i_varecka
  gplend

ICON i_varecka_zlomena
   title połamana łyżka
  STATUS off
  PICTURE ..\ikony\def\08.gcf
  look
   D: "Te dwa kawałki drewna
       na nic mi się na zdadzą."
  gplend
  canuse (isactico(i_provaz))
  use
   D: "Drewnianą łyżkę kuchenną
       można naprawić sznurkiem..."
    icostat off i_varecka_zlomena
    icostat off i_provaz
    icostat on i_varecka
  gplend

